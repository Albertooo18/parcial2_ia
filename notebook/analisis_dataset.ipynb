
import pandas as pd
import matplotlib.pyplot as plt
import seaborn as sns

url = "https://raw.githubusercontent.com/Albertooo18/parcial2_ia/main/dataset/trafico_red.csv"

try:
    # Cargar el dataset
    df = pd.read_csv(url)
    print("Dataset cargado correctamente.\n")
    print(df.head())

    # Seleccionar solo columnas num√©ricas
    df_numeric = df.select_dtypes(include=['number'])

    # Estad√≠sticas b√°sicas
    print("\nEstad√≠sticas b√°sicas:")
    estadisticas = pd.DataFrame({
        "Media": df_numeric.mean(),
        "Mediana": df_numeric.median(),
        "Moda": df_numeric.mode().iloc[0],
        "Desviaci√≥n est√°ndar": df_numeric.std()
    })
    print(estadisticas)

    # Matriz de correlaci√≥n
    print("\nüîó Matriz de correlaci√≥n entre variables num√©ricas:")
    correlacion = df_numeric.corr()
    print(correlacion)

    # Visualizar matriz de correlaci√≥n
    plt.figure(figsize=(10, 6))
    sns.heatmap(correlacion, annot=True, cmap='coolwarm', fmt=".2f")
    plt.title("Matriz de correlaci√≥n entre variables num√©ricas")
    plt.tight_layout()
    plt.show()

except Exception as e:
    print("‚ùå Error al cargar el dataset:", e)
